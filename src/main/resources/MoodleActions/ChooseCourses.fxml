<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="600" prefWidth="600" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ChooseCourses">
   <top>
      <Label text="Choose Courses">
         <font>
            <Font size="30.0" />
         </font></Label>
   </top>
   <center>
      <TreeView fx:id="coursesTree" />
   </center>
   <bottom>
      <Button alignment="CENTER" contentDisplay="CENTER" defaultButton="true" onAction="#nextScene" text="Next" />
   </bottom>
</BorderPane>
