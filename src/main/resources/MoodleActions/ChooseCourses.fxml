<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ChooseCourses">
   <Label text="Choose what you want to download." GridPane.rowIndex="0" />
   <Label text="Everything is selected by default" GridPane.rowIndex="1" />
   <VBox fx:id="checkboxes" GridPane.rowIndex="2" />
   <Button onAction="#nextScene" text="Next" GridPane.rowIndex="3" />
</GridPane>
